page{background-color:#fff}.place-guide{display:-webkit-flex;display:flex;height:auto;padding:32rpx;width:auto}.place-guide image{flex:1}.place-guide .slider-wrapper{background-color:#e3e3e3;border-radius:23px;height:auto;margin:0 32rpx;position:relative;width:46px}.place-guide .slider-wrapper .indicator{background-color:#ff7900;border-radius:23px;height:46px;position:relative;top:46.1%;width:46px;z-index:888}.place-guide .slider-wrapper .indicator.success{background-color:#07c160;color:#fff;font-size:23px;line-height:46px;text-align:center}.place-guide .slider-wrapper .slider{background-color:#f3f3f3;border:1rpx solid #ff4900;border-radius:23px;box-sizing:border-box;color:#ff4900;height:46px;line-height:46px;position:relative;text-align:center;top:0;width:46px;z-index:999}.place-guide .slider-wrapper .slider .iconfont{font-size:24px!important}.desc{color:#555;font-size:28rpx;line-height:150%;padding:32rpx 36rpx}